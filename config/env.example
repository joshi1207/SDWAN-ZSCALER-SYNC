# vManage connection
VMANAGE_HOST=192.168.1.67
VMANAGE_PORT=8443
VMANAGE_USER=api-user
VMANAGE_PASS=changeme

# Base Data Prefix List name
DPL_NAME=ZSCALER_BYPASS

# Zscaler ZPA JSON URL (example for zscaler.net cloud)
ZSCALER_JSON_URL=https://config.zscaler.com/api/zscaler.net/zpa/json

# Paths (relative or absolute)
CACHE_FILE=cache.json
BACKUP_DIR=backups
LOG_LEVEL=INFO

# Safety guard: % of entries allowed to be removed vs last cache
MAX_REMOVE_PERCENT=25

# TLS verification for HTTPS (true/false)
VERIFY_TLS=false

# IP family to import: ipv4, ipv6, both
ZSCALER_FAMILY=ipv4

# Max number of entries per Data Prefix List
ZSCALER_MAX_CHUNK=500

# Optional: Microsoft Teams webhook
TEAMS_WEBHOOK_URL=
